{
  "name": "autoform",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "cypress": "turbo run cypress --filter=web",
    "cypress:dev": "turbo run cypress --filter=web --parallel"
  },
